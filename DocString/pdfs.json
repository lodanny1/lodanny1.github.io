{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "UploadsWriteAndMultipart",
      "Effect": "Allow",
      "Action": [
        "s3:PutObject",
        "s3:AbortMultipartUpload",
        "s3:ListBucketMultipartUploads",
        "s3:ListMultipartUploadParts"
      ],
      "Resource": [
        "arn:aws:s3:::severless-proj-uploads-251985477703-us-west-1/*",
        "arn:aws:s3:::severless-proj-uploads-251985477703-us-west-1"
      ]
    },
    {
      "Sid": "DownloadsIfYouPresignGET",
      "Effect": "Allow",
      "Action": ["s3:GetObject"],
      "Resource": "arn:aws:s3:::severless-proj-uploads-251985477703-us-west-1/*"
    }
  ]
}
